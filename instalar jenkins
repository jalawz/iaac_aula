sudo sh -c 'echo deb http://pkg.jenkins.io/debian-stable binary/ > /etc/apt/sources.list.d/jenkins.list'\n
sudo apt update
sudo apt install jenkins
sudo apt install openjdk-11-jdk 
java --version
sudo systemctl start jenkins 
